        <contributions-group>
      	<contributions status="past" from="2008-04-04" position="Developer"/>
      	</contributions-group>	
    </author>
    </author-group>
	<mod-version>1.0.2</mod-version>
    <installation>
      <level>easy</level>
      <time>600</time>
	<target-version>3.0.8</target-version>
    </installation>
